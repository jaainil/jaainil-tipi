{
    "$schema": "https://raw.githubusercontent.com/runtipi/runtipi/main/packages/shared/src/schemas/app-info.json",
    "id": "ackee",
    "available": true,
    "name": "Ackee",
    "short_desc": "Self-hosted analytics tool with full data ownership",
    "description": "Ackee is a self-hosted analytics platform that focuses on privacy, simplicity, and transparency. It helps you understand how users interact with your website without tracking them across the web.",
    "author": "Electerious",
    "source": "https://github.com/electerious/Ackee",
    "website": "https://ackee.electerious.com",
    "tipi_version": 4,
    "version": "latest",
    "port": 3000,
    "categories": [
        "data",
        "development"
    ],
    "exposable": true,
    "https": false,
    "supported_architectures": [
        "amd64",
        "arm64"
    ],
    "form_fields": [
        {
            "type": "text",
            "label": "Admin Username",
            "placeholder": "admin",
            "required": true,
            "env_variable": "ACKEE_USERNAME"
        },
        {
            "type": "password",
            "label": "Admin Password",
            "required": true,
            "env_variable": "ACKEE_PASSWORD"
        }
    ]
}