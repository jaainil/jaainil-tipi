{
    "schemaVersion": 2,
    "services": [
        {
            "name": "browserless",
            "image": "ghcr.io/browserless/chromium:latest",
            "isMain": true,
            "internalPort": 3000,
            "environment": {
                "TOKEN": ""
            },
            "healthCheck": {
                "test": "CMD curl -f http://127.0.0.1:3000/docs",
                "interval": "2s",
                "timeout": "10s",
                "retries": 15
            }
        }
    ]
}